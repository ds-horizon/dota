{
  "name": "dota",
  "version": "0.0.1",
  "description": "Source code for the CodePush service API",
  "main": "./script/server.js",
  "scripts": {
    "start": "node ./bin/script/server.js",
    "start:env": "node -r dotenv/config ./bin/script/server.js dotenv_config_path=.env dotenv_config_silent=true",
    "clean": "rimraf bin",
    "build": "tsc && shx cp -r ./script/views ./bin/script",
    "restart": "npm run build && npm run start:env",
    "prettier": "prettier --write \"./**/*.ts\"",
    "lint": "npx eslint ./script/**/*.ts",
    "lint:fix": "npx eslint ./script/**/*.ts --fix",
    "test": "jest --testPathIgnorePatterns='test/management-new.test.ts' 'test/utils.test.ts'",
    "azure:setup": "./runazure.sh",
    "azure:init": "npx azurite -s -l $AZURITE_DIR --tableHost $AZURITE_TABLE_HOST --tablePort $AZURITE_TABLE_PORT --queueHost $AZURITE_QUEUE_HOST --queuePort $AZURITE_QUEUE_PORT --blobHost $AZURITE_BLOB_HOST --blobPort $AZURITE_BLOB_PORT",
    "azure:seed": "npx ts-node -r dotenv/config script/storage/seedDataAzure.ts",
    "azure:dev": "npm run azure:seed && npm run build && npm run start:env",
    "aws:setup": "./runaws.sh --setup-only",
    "aws:run": "./runaws.sh --run-only",
    "dota:cli": "./runaws.sh",
    "aws:init": "docker compose up",
    "aws:seed": "npx ts-node -r dotenv/config script/storage/seedData.ts",
    "aws:dev": "npm run aws:seed && npm run build && npm run start:env",
    "aws:clean": "docker compose down -v",
    "json:setup": "./runlocal.sh",
    "dev": "node -r dotenv/config ./runsetup.sh",
    "dev:web": "node -r dotenv/config ./runsetup.sh --web"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "author": "Dream Sports Labs <dreamsportslabs@dream11.com> (https://github.com/ds-horizon)",
  "dependencies": {
    "@azure/data-tables": "^13.3.0",
    "@azure/identity": "^4.2.1",
    "@azure/keyvault-secrets": "^4.7.0",
    "@azure/storage-blob": "^12.27.0",
    "@azure/storage-queue": "^12.13.0",
    "applicationinsights": "^2.7.0",
    "aws-cloudfront-sign": "^3.0.2",
    "aws-sdk": "2.1691.0",
    "blocked-at": "^1.2.0",
    "body-parser": "^1.20.2",
    "cookie-session": "^2.0.0",
    "dd-trace": "^5.36.0",
    "ejs": "^3.1.10",
    "email-validator": "1.0.3",
    "express": "^4.21.2",
    "express-domain-middleware": "0.1.0",
    "express-rate-limit": "^7.5.0",
    "google-auth-library": "9.9.0",
    "ioredis": "5.4.0",
    "lusca": "^1.7.0",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "3.11.3",
    "nanoid": "5.0.7",
    "node-deepcopy": "0.1.1",
    "passport": "^0.6.0",
    "passport-azure-ad": "^4.3.5",
    "passport-github2": "0.1.9",
    "passport-http-bearer": "1.0.1",
    "passport-windowslive": "1.0.1",
    "redis": "2.4.2",
    "sanitize-html": "^2.11.0",
    "semver": "^7.5.3",
    "sequelize": "6.37.4",
    "shortid": "2.2.2",
    "stream-to-array": "^2.3.0",
    "streamifier": "0.1.1",
    "superagent": "^8.0.9",
    "try-json": "1.0.0",
    "wordwrap": "^1.0.0",
    "yauzl": "2.6.0",
    "yazl": "2.2.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.1",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.1.4",
    "@types/q": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^16.4.7",
    "eslint": "^8.45.0",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "shx": "^0.3.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
